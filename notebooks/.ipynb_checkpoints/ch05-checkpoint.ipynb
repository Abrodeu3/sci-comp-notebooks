   "source": [
    "function bisection(f::Function,a::Real,b::Real)\n",
    "  local c\n",
    "  while (b-a)>1e-6\n",
    "    c = 0.5*(a+b)\n",
    "    if f(a)*f(c) < 0 \n",
    "      b=c\n",
    "    else\n",
    "      a=c\n",
    "    end\n",
    "  end\n",
    "  c\n",
    "end"
   ]
